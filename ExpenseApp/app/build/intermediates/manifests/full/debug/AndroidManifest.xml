<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="inclass11.group05.com.expenseapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="24" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="inclass11.group05.com.expenseapp.Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="inclass11.group05.com.expenseapp.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="inclass11.group05.com.expenseapp.LoginActivity" />
        <activity android:name="inclass11.group05.com.expenseapp.SignUpActivity" />
        <activity android:name="inclass11.group05.com.expenseapp.ExpensesList" />
        <activity android:name="inclass11.group05.com.expenseapp.ExpenseDetailActivity" />
        <activity android:name="inclass11.group05.com.expenseapp.AddExpenseActivity" />
    </application>

</manifest>