<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.inclass8siddhant.ShowExpense">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/editTextShow"
        android:text="Expense details"
        android:layout_marginLeft="120dp"
        android:textStyle="normal|bold" />

    <TextView
        android:text="Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView3"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="20dp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewShowName"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="120dp" />

    <TextView
        android:text="Category"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        android:layout_marginTop="150dp"
        android:layout_marginLeft="20dp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewShowCategory"
        android:layout_marginTop="150dp"
        android:layout_marginLeft="120dp" />

    <TextView
        android:text="Amount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView8"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="20dp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewShowAmount"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="120dp" />

    <TextView
        android:text="Date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView10"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="250dp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewShowDate"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="250dp" />

    <Button
        android:text="Close"
        android:layout_height="wrap_content"
        android:id="@+id/buttonClose"
        android:layout_width="wrap_content"
        android:layout_marginTop="350dp"
        android:layout_marginLeft="120dp" />

</FrameLayout>
